package com.company;

public class Main {

    static void universal_sum(String a, String b) {
        double d = Double.parseDouble(a);
        double f = Double.parseDouble(b);
        double sum = d + f;
        System.out.println(sum);
    }

    public static void main(String[] args) {
    
        String a = Double.toString(0.5);
        String b = Integer.toString(7);
        universal_sum(a, b);
    }
}
