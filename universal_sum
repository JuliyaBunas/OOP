package com.company;

public class Main {

    static double universal_sum(Number a, Number b) {
        double f = Double.parseDouble(a.toString()); //по-другому вытянуть значение не получилось?
        double g = Double.parseDouble(b.toString()); //не было метода doubleValue()?
        return (f + g);
    }

    public static void main(String[] args) {

        Number a = 15;
        Number b = 25.45;
        universal_sum(a, b);
        System.out.println(universal_sum(a, b));

    }
}
